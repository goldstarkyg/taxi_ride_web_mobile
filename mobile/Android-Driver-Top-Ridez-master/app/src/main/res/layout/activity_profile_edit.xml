                    android:src="@color/selected_indicator"
                    android:layout_centerHorizontal="true"
                    android:scaleType="fitXY"
                    android:visibility="gone"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ll_legal_details_inactive_step_frth"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="visible">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_87"
                    android:background="@color/yellow">

                    <ImageView
                        android:id="@+id/img_legla_inactive_step_frth"
                        android:layout_width="@dimen/width_40"
                        android:layout_height="@dimen/height_40"
                        android:src="@drawable/legal_active"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/margin_10"
                        />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_25"
                    android:layout_marginTop="@dimen/margin_10"
                    android:layout_alignParentBottom="true">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/height_10"
                        android:layout_alignParentBottom="true"
                        android:background="@android:color/transparent">

                    </RelativeLayout>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/height_25"
                        android:src="@drawable/icon_done"
                        android:layout_centerInParent="true"
                        android:visibility="visible"/>



                </RelativeLayout>
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@color/selected_indicator"
                    android:layout_centerHorizontal="true"
                    android:scaleType="fitXY"
                    android:visibility="gone"/>

            </RelativeLayout>




            <RelativeLayout
                android:id="@+id/ll_bank_details_active_step_frth"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.2"
                android:visibility="visible">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_87"
                    android:background="@color/yellow"
                    >

                    <ImageView
                        android:id="@+id/img_bank_step_frth"
                        android:layout_width="@dimen/width_40"
                        android:layout_height="@dimen/height_40"
                        android:src="@drawable/bank"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/margin_10"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bank_details"
                        android:textSize="@dimen/font_size_16sp"
                        android:textColor="@color/black"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/margin_5"
                        android:layout_below="@+id/img_bank_step_frth"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_25"
                    android:layout_marginTop="@dimen/margin_10"
                    android:layout_alignParentBottom="true"
                    android:visibility="visible">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/height_10"
                        android:layout_alignParentBottom="true"
                        android:background="@android:color/transparent"
                        android:visibility="gone">

                    </RelativeLayout>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/height_25"
                        android:src="@drawable/icon_done"
                        android:layout_centerInParent="true"
                        android:visibility="gone"/>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@color/selected_indicator"
                        android:layout_centerHorizontal="true"
                        android:scaleType="fitXY"
                        android:visibility="visible"
                        android:layout_centerVertical="true"/>

                </RelativeLayout>


            </RelativeLayout>

        </LinearLayout>
        <!-- Layout fourth End -->

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout_header"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_90"
        android:background="@color/yellow"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:id="@+id/layout_back_next"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/layout_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:padding="@dimen/margin_10"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/img_prev"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height_20"
                    android:src="@drawable/prev"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/back"
                    android:textColor="@color/black"
                    android:textSize="@dimen/font_size_16sp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/img_prev"
                    android:layout_marginLeft="@dimen/margin_5"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_next"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_alignParentRight="true"
                android:padding="@dimen/margin_10">


                <TextView
                    android:id="@+id/txt_next"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/font_size_16sp"
                    android:text="@string/next"
                    android:layout_marginRight="@dimen/margin_5" />

                <ImageView
                    android:id="@+id/img_next"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height_20"
                    android:src="@drawable/next"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/txt_next"/>

            </RelativeLayout>

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_caption_signup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/caption_edit_prf"
            android:textColor="@color/cSignUP"
            android:textSize="@dimen/font_size_25sp"
            android:layout_centerHorizontal="true"
            android:layout_margin="@dimen/margin_10"
            android:layout_below="@+id/layout_back_next"
            android:singleLine="true"/>
    </RelativeLayout>

    <include
        android:id="@+id/vAlert"
        layout="@layout/notify_alert"></include>

</RelativeLayout>
